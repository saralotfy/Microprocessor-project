        INT 21H
        
        LEA DX,MF2
        MOV AH,9h
        INT 21H
              
        LEA DX,M38              
        MOV AH,9h
        INT 21H 
              
        MOV AH,1h
        INT 21H
        MOV BL,AL
        SUB BL,48 
        
        CMP BL,1
        JE FIVE
        
        CMP BL,2
        JE FIVE
        
        CMP BL,3
        JE TEN 
        
        CMP BL,4
        JE TEN
        
        CMP BL,5
        JE TEN
        
        CMP BL,6
        JE TEN
        
        
        JMP INVALID
        
APPETIZERS:
    
        LEA DX,M7    
        MOV AH,9h
        INT 21H
        
        LEA DX,NEWLINE 
        MOV AH,9h
        INT 21H
        
        LEA DX,MF2
        MOV AH,9h
        INT 21H
               
        LEA DX,MF2
        MOV AH,9h
        INT 21H
              
        LEA DX,M32    
        MOV AH,9h
        INT 21H 
                
        LEA DX,M33  
        MOV AH,9h
        INT 21H
        
        LEA DX,M34
        MOV AH,9h          
        INT 21H 
        
        LEA DX,M35
        MOV AH,9h           
        INT 21H
               
        LEA DX,M45
        MOV AH,9h           
        INT 21H       
               
               
        LEA DX,M46
        MOV AH,9h           
        INT 21H 
                     
                                 
               
        LEA DX,MF2
        MOV AH,9h
        INT 21H
        
        LEA DX,MF2
        MOV AH,9h
        INT 21H
              
        LEA DX,M38              
        MOV AH,9h
        INT 21H 
              
        MOV AH,1h
        INT 21H
        MOV BL,AL
        SUB BL,48           
                   
        CMP BL,1
        JE TEN
        
        CMP BL,2
        JE TEN
        
        CMP BL,3
        JE TEN 
        
        CMP BL,4
        JE TEN
        
        CMP BL,5
        JE TEN
        
        CMP BL,6
        JE TEN
        
        
        JMP INVALID               
                   

    Calculate: 
     
        LEA DX,M39              
        MOV AH,9
        INT 21H 
           
        MOV AH,1
        INT 21H
        SUB AL,48
        mov ah,00          
        
        MUL BL
        
        ADD PRC,AX
        
        ret

main endp
end main
