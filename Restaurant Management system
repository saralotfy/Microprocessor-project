.CODE
MAIN PROC

    MOV AX,@DATA
    MOV DS,AX
    
  TOP:
  
    LEA DX,M1
    MOV AH,9h
    INT 21H
    
    LEA DX,NEWLINE 
    MOV AH,9h
    INT 21H
    
    LEA DX,MF1
    MOV AH,9h
    INT 21H
       
    LEA DX,MF1
    MOV AH,9h
    INT 21H  
        
    LEA DX,M3
    MOV AH,9h
    INT 21H
    
    LEA DX,M4
    MOV AH,9h
    INT 21H
    
    LEA DX,M5
    MOV AH,9h
    INT 21H
    
    LEA DX,M6
    MOV AH,9h
    INT 21H
       
    LEA DX,MF1
    MOV AH,9h
    INT 21H
    
    LEA DX,MF1
    MOV AH,9h
    INT 21H
    EE1: 
    
   EE1:
    
        LEA DX,NEWLINE
        MOV AH,9h
        INT 21H
         
        LEA DX,M2
        MOV AH,9h
        INT 21H      
         
        MOV AH,1h
        INT 21H
        MOV BH,AL
        SUB BH,48
        
        CMP BH,1
        JE BREATFAST
        
         CMP BH,2
         JE DINNER   
         
         CMP BH,3
         JE APPETIZERS 
                
         CMP BH,4
         JE DRINKS
        
        LEA DX,M36
        MOV AH,9
        INT 21H 
            
            
        LEA DX,M37 
        MOV AH,9
        INT 21H
        
        loop EE1      
       
   BREATFAST:
          
        LEA DX,M7    
        MOV AH,9h
        INT 21H
        
        LEA DX,NEWLINE 
        MOV AH,9h
        INT 21H
        
        LEA DX,MF2
        MOV AH,9h
        INT 21H
               
        LEA DX,MF2
        MOV AH,9h
        INT 21H
              
        LEA DX,M8    
        MOV AH,9h
        INT 21H 
                
        LEA DX,M9  
        MOV AH,9h
        INT 21H
        
        LEA DX,M10
        MOV AH,9h          
        INT 21H 
        
        LEA DX,M11
        MOV AH,9h           
        INT 21H
                
        LEA DX,M12         
        MOV AH,9h
        INT 21H
               
        LEA DX,M13     
        MOV AH,9h
        INT 21H 
                        
        LEA DX,M14
        MOV AH,9h           
        INT 21H
                                
        LEA DX,M15        
        MOV AH,9h
        INT 21H 
        
        LEA DX,M16         
        MOV AH,9h
        INT 21H
               
        LEA DX,MF2
        MOV AH,9h
        INT 21H
        
        LEA DX,MF2
        MOV AH,9h
        INT 21H
              
         EE2:
        
            LEA DX,M38              
            MOV AH,9h
            INT 21H 
            
            
            MOV AH,1h
            INT 21H
            MOV BL,AL
            SUB BL,48 
            
            CMP BL,1
            JE TEN
            
            CMP BL,2
            JE TEN
            
            CMP BL,3
            JE TEN 
            
            CMP BL,4
            JE TEN
            
            CMP BL,5
            JE TEN
            
            CMP BL,6
            JE TWENTY
            
            CMP BL,7
            JE TEN
            
            CMP BL,8
            JE TWENTY 
            
            CMP BL,9
            JE SIXTY
        
            
            LEA DX,M36
            MOV AH,9
            INT 21H 
                
                
            LEA DX,M37 
            MOV AH,9
            INT 21H
            loop EE2
            
    
        
  DINNER:
          
        LEA DX,M7    
        MOV AH,9h
        INT 21H
        
        LEA DX,NEWLINE 
        MOV AH,9h
        INT 21H
        
        LEA DX,MF2
        MOV AH,9h
        INT 21H
               
        LEA DX,MF2
        MOV AH,9h
        INT 21H
              
        LEA DX,M17    
        MOV AH,9h
        INT 21H 
                
        LEA DX,M18  
        MOV AH,9h
        INT 21H
        
        LEA DX,M19
        MOV AH,9h          
        INT 21H 
        
        LEA DX,M20
        MOV AH,9h           
        INT 21H
                
        LEA DX,M21         
        MOV AH,9h
        INT 21H
               
        LEA DX,M22     
        MOV AH,9h
        INT 21H 
                        
        LEA DX,M23
        MOV AH,9h           
        INT 21H
                                
        LEA DX,M24        
        MOV AH,9h
        INT 21H 
        
        LEA DX,M25        
        MOV AH,9h
        INT 21H
               
        LEA DX,MF2
        MOV AH,9h
        INT 21H
        
        LEA DX,MF2
        MOV AH,9h
        INT 21H
              
        LEA DX,M38              
        MOV AH,9h
        INT 21H 
              
        MOV AH,1h
        INT 21H
        MOV BL,AL
        SUB BL,48      
        CMP BL,1
        JE SIXTY

        CMP BL,2
        JE EIGHTY

        CMP BL,3
        JE EIGHTY 

        CMP BL,4
        JE FOURTY

        CMP BL,5
        JE FIFTY

        CMP BL,6
        JE SEVENTY

        CMP BL,7
        JE SIXTY

        CMP BL,8
        JE SIXTY 

        CMP BL,9
        JE SIXTY

   Drinks:
          
        LEA DX,M43    
        MOV AH,9h
        INT 21H
        
        LEA DX,NEWLINE 
        MOV AH,9h
        INT 21H
        
        LEA DX,MF2
        MOV AH,9h
        INT 21H
               
        LEA DX,MF2
